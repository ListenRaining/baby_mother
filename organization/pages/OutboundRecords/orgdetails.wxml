<view class="content_wrap">
    <form bindsubmit='formSubmit'>
      <view class="top_text">个人信息</view>

       <view class="li verificationLi">
            <label class= "title">联系人</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="张老师" name="ins_name" value="{{ins_name}}"/>
          </view>
           <view class="li verificationLi">
            <label class= "title">联系电话</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="15120008674" name="ins_name" value="{{ins_name}}"/>
          </view>


      <view class="top_text">机构基本信息</view>
 
          <view class="li verificationLi">
            <label class= "title">机构名称</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="长歌行艺术馆" name="ins_name" value="{{ins_name}}"/>
          </view>

           <view class="li verificationLi">
                <label class= "title">机构类型</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="艺术培训机构" name="ins_name" value="{{ins_name}}"/>
          </view>

          <view class="li verificationLi">
                <label class= "title">机构地址</label>

            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="北京市 朝阳区" name="ins_name" value="{{ins_name}}"/>
       </view>

          <view class="li verificationLi">
            <label class= "title">机构层高</label>
            <input class= "Number" placeholder-class="phcolor" placeholder="3m" name="ins_name" value="{{ins_name}}"/>
          </view>
          <view class="li verificationLi">
                <label class= "title">总面积</label>

                 <input class= "Number" placeholder-class="phcolor" placeholder="100㎡以下" name="ins_name" value="{{ins_name}}"/>
          </view>

           <view class="li verificationLi">
                <label class= "title">租赁期</label>
                <input class= "Number" placeholder-class="phcolor" placeholder="请输入租赁 5年" name= "code" />
               
           </view>

           <view class="li verificationLi">
                <label class= "title">发票类型</label>
                  <input class= "Number" placeholder-class="phcolor" placeholder="增值税专用发票" name= "code" />
          </view>

          <view class="li verificationLi">
                <label class= "title">经营年限</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="1-3年" name= "code" />
          </view>

          <view class="top_text">机构员工信息</view>
          
          <view class="li verificationLi">
                <label class= "title">教务人数</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="3-5人" name= "code" />
          </view>
          
          <view class="li verificationLi">
                <label class= "title">教师人数</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="3-5人" name= "code" />
          </view>

          <view class="top_text">机构生源信息</view>
          
          <view class="li verificationLi">
                <label class= "title">年龄段</label>
                  <input class= "Number" placeholder-class="phcolor" placeholder="3-6岁" name= "code" />

          </view>

          <view class="li verificationLi">
                <label class= "title">人数</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="3-6岁" name= "code" />
          </view>


          <view class="top_text">机构教室信息</view>
          
          <view class="li verificationLi">
                <label class= "title">教室数量</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="4间" name= "code" />
          </view>

          <view class="li verificationLi">
                <label class= "title">所在楼层</label>
                <input class= "Number" placeholder-class="phcolor" placeholder="18层" name= "code" />
                
           </view>

           <view class="li verificationLi">
                <label class= "title">教室长</label>
                <input class= "Number" placeholder-class="phcolor" placeholder="100m" name= "code" />
               
           </view>

           <view class="li verificationLi">
                <label class= "title">教室宽</label>
                <input class= "Number" placeholder-class="phcolor" placeholder="80m" name= "code" />
               
           </view>


            <view class="li verificationLi">
                <label class= "title">承重墙</label>

                 <input class= "Number" placeholder-class="phcolor" placeholder="隔断墙有龙骨" name= "code" />
          </view>

           <view class="li verificationLi">
                <label class= "title">网络</label>

                <input class= "Number" placeholder-class="phcolor" placeholder="隔断墙有龙骨" name= "code" />
          </view>

          <view class="li verificationLi">
                <label class= "title">带宽</label>
                
                <input class= "Number" placeholder-class="phcolor" placeholder="有线网络 100M" name= "code" />
               
           </view>
            
             <view class="li verificationLi">
                <label class= "title">闲置时间</label>
               <input class= "Number" placeholder-class="phcolor" placeholder="周末全天" name= "code" />
          </view>

      

        <view class="top_text">周边信息</view>

       <view class="li verificationLi">
            <label class= "title">交通状况</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="" name="ins_name" value="{{ins_name}}"/>
         </view>

         <view class="li verificationLi">
            <label class= "title">基础设施</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="" name="ins_name" value="{{ins_name}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">周边社区</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="" name="ins_name" value="{{ins_name}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">常住人口</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="" name="ins_name" value="{{ins_name}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">同类机构</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="" name="ins_name" value="{{ins_name}}"/>
         </view>

         <view class="li verificationLi">
            <label class= "title">幼儿园</label>
            <input class= "Number" placeholder-class="phcolor" placeholder-class="phcolor"   placeholder="" name="ins_name" value="{{ins_name}}"/>
         </view>

       

        <!-- <button class='.button' form-type='submit'>保存</button> -->
    </form>
    
</view>
 <view class='bottom'></view>