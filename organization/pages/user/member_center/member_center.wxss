page {
  background-color: #fff;
}

.banner_img {
  width: 100%;
}

.ul {
  display: flex;
  flex-direction: column;
}

.ul .item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 33rpx 30rpx;
  border-bottom: solid 1rpx #f0f0f0;
  font-size: 30rpx;
  background-color: #fff;
}

.ul .item .msg {
  margin-left: auto;
}

.ul .item .arrow {
  width: 15rpx;
  flex-shrink: 0;
  margin-left: 28rpx;
}

.adr{
  overflow: hidden;
  white-space: nowrap;
  text-overflow:clip;
}

.exadr{
  width:250rpx;
}