<view class="content_wrap">
    <form bindsubmit='formSubmit'>
       <view class="top_text">
        <view class="distance">个人信息</view>         
      </view>
       <view class="li verificationLi">
            <label class= "title">联系人</label>
             <label class= "Number">{{info.username}}</label>
          </view>
           <view class="li_bootom verificationLi">
            <label class= "title">联系电话</label>
            <label class= "Number">{{info.mobile}}</label>
            
          </view>

      <view class="top_text">
        <view class="distance">机构基本信息</view>   
        </view>
 
          <view class="li verificationLi">
            <label class= "title">机构名称</label>
             <label class= "Number">{{info.ins_name}}</label>
          
          </view>

           <view class="li verificationLi">
                <label class= "title">机构类型</label>
                 <label class= "Number">{{info.ins_name}}</label>
                
          </view>

          <view class="li verificationLi">
                <label class= "title">机构地址</label>
                <label class= "Number">{{info.address}}</label>
       </view>

          <view class="li verificationLi">
            <label class= "title">机构层高</label>
             <label class= "Number">{{info.ins_high}}</label>
          </view>

          <view class="li verificationLi">
                <label class= "title">总面积</label>
                <label class= "Number">{{info.ins_area}}</label>
          </view>

           <view class="li verificationLi">
                <label class= "title">场地性质</label>
                <label class= "Number">{{info.site_properties}}</label>
           </view>

           

           <view class="li verificationLi">
                <label class= "title">发票类型</label>
                <label class= "Number">{{info.invoice_type}}</label>
          </view>
          <view class="li_bootom verificationLi">
                <label class= "title">经营年限</label>
                 <label class= "Number">{{info.leasehold}}</label>
           </view>

           <view class="top_text">
           <view class="distance">机构生源信息</view>   
          </view>
          
          <view class="li verificationLi">
                <label class= "title">教务人数</label>
                <label class= "Number">{{info.edu_num}}</label>
                
          </view>
          <view class="li_bootom verificationLi">
                <label class= "title">教师人数</label>
                <label class= "Number">{{info.tea_num}}</label>
                
          </view>
        
          <view class="top_text">机构生源信息</view>
          
          <view class="li verificationLi">
                <label class= "title">年龄段</label>
                 <label class= "Number">{{info.agestr}}</label>
              

          </view>

          <view class="li_bootom verificationLi">
              <label class= "title">人数</label>
               <label class= "Number">{{info.students}}</label>
              <input class= "Number" placeholder-class="phcolor" value='{{info.students}}' />
          </view>

         <view class="top_text">
           <view class="distance">机构教室信息</view>   
          </view>
          
          <view class="li verificationLi">
                <label class= "title">教室数量</label>
                <label class= "Number">{{info.classroom_num}}</label>
              

           <view class="li verificationLi">
                <label class= "title">所在楼层</label>
                 <label class= "Number">{{info.room_floor}}</label>
                <input class= "Number" placeholder-class="phcolor" value='{{info.room_floor}}' />
           </view>

           <view class="li verificationLi">
                <label class= "title">教室长</label>
                <label class= "Number">{{info.room_long}}</label>
                <input class= "Number" placeholder-class="phcolor" value="{{info.room_long}}"/>
           </view>

           <view class="li verificationLi">
                <label class= "title">教室宽</label>
                 <label class= "Number">{{info.room_wide}}</label>
            
           </view>

            <view class="li verificationLi">
                <label class= "title">承重墙</label>
                 <label class= "Number">{{info.ins_wall}}</label>
              
            </view>

           <view class="li verificationLi">
                <label class= "title">网络</label>
                 <label class= "Number">{{info.network}}</label>
              
          </view>

          <view class="li verificationLi">
                <label class= "title">带宽</label>
                 <label class= "Number">{{info.broadband}}</label>
            
           </view>
            
             <view class="li_bootom verificationLi">
                <label class= "title">闲置时间</label>
                <label class= "Number">{{info.idle_time}}</label>
          </view>

        <view class="top_text">
           <view class="distance">周边信息</view> 
        </view>

       <view class="li verificationLi">
            <label class= "title">交通状况</label>
             <label class= "Number">{{info.transport}}</label>
         </view>

         <view class="li verificationLi">
            <label class= "title">基础设施</label>
             <label class= "Number">{{info.infrastructure}}</label>
          
         </view>
         <view class="li verificationLi">
            <label class= "title">周边社区</label>
             <label class= "Number">{{info.community}}</label>
            
         </view>
         <view class="li verificationLi">
            <label class= "title">常住人口</label>
             <label class= "Number">{{info.inhabitant}}</label>
         
         </view>
         <view class="li verificationLi">
            <label class= "title">同类机构</label>
            <label class= "Number">{{info.peer_group}}</label>
           
         </view>
         <view class="li verificationLi">
            <label class= "title">幼儿园</label>
             <label class= "Number">{{info.kindergarten}}</label>
         </view>
         </view>
    </form>
    
</view>
 <view class='bottom'></view>