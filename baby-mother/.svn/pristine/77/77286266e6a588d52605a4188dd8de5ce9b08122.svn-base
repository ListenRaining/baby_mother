page{
  background: #ffffff;
}

.tab{
  width: 100%;
  font-size:28rpx;
  border-bottom: 1px solid #f0eff4;
  border-top: 2rpx solid #f0eff4;
}

.tab view{
  display: inline-block;
  width: 50%;
  text-align: center;
  line-height: 74rpx;
  font-size: 30rpx;
  color: #a9a9a9;
}

.newUser,.intenUser{
  margin: 0 25rpx;
  padding-bottom: 155rpx;
}

.item{
  height: 154rpx;
  border-bottom: 2rpx solid #e9e9e9;
  padding-left: 40rpx;
}

.img_box{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
  margin-top: 17rpx;
}

.img_box image{
  width: 100%;
  max-height: 100%;
}

.text_box{
  display: inline-block;
  padding-left: 30rpx;
}

.title{
  padding-top: 33rpx;
  font-size: 30rpx;
}

.time{
  font-size: 22rpx;
  color: #999999;
  padding-top: 13rpx;
}

.item2{
  height: 154rpx;
  border-bottom: 2rpx solid #e9e9e9;
  padding-left: 40rpx;
}

.title2{
  padding-top: 26rpx;
  font-size: 30rpx;
}

.time2{
  font-size: 22rpx;
  color: #999999;
  padding-top: 4rpx;
}

.check_box{
  float: right;
  margin-top: 63rpx;
}

.none{
  display: none;
}

.tab .active{
  border-bottom: 4rpx solid #41a638;
  color:#41a638;
}

.send{
  width: 100%;
  height: 155rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #ffffff;
}

.send .btn{
  width: 530rpx;
  height: 80rpx;
  display: inline-block;
  background: #41a638;
  color: #ffffff;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  top: 33rpx;
  margin-left: -290rpx;
}

.send .checkbox{
  position: absolute;
  right: 25rpx;
  top: 33rpx;
}
.tab_after{
  position:relative;
}
.tab_after::after{
  content: '';
  position:absolute;
  width: 2rpx;
  height: 30rpx;
  background: #e9e9e9;
  right: 0;
  top: 23rpx;
}