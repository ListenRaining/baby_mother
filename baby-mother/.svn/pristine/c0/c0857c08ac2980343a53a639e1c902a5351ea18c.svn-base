                        <icon type="clear" size="20" color="gray" bindtap='delChartertoImg' />
                    </image>
                    <image bindtap='addChartertoImg' clas="addImg_item" mode="aspectFit" class="add_image" src="../../../image/icon-play/addImage.png"></image>
