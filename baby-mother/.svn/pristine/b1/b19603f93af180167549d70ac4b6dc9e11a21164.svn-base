<view class="content_wrap">
    <form bindsubmit='formSubmit'>
       <view class="top_text">
        <view class="distance">个人信息</view>         
      </view>
       <view class="li verificationLi">
            <label class= "title">联系人</label>
            <input class= "Number" placeholder-class="phcolor"  value="{{info.username}}"/>
          </view>
           <view class="li_bootom verificationLi">
            <label class= "title">联系电话</label>
            <input class= "Number" placeholder-class="phcolor"   value="{{info.mobile}}"/>
          </view>

      <view class="top_text">
        <view class="distance">机构基本信息</view>   
        </view>
 
          <view class="li verificationLi">
            <label class= "title">机构名称</label>
            <input class= "Number"  placeholder-class="phcolor"  value="{{info.ins_name}}"/>
          </view>

           <view class="li verificationLi">
                <label class= "title">机构类型</label>
                 <input class= "Number" placeholder-class="phcolor"  value="{{info.ins_name}}"/>
          </view>

          <view class="li verificationLi">
                <label class= "title">机构地址</label>

            <input class= "Number" placeholder-class="phcolor"   value="{{info.address}}"/>
       </view>

          <view class="li verificationLi">
            <label class= "title">机构层高</label>
            <input class= "Number" placeholder-class="phcolor"  value="{{info.ins_high}}"/>
          </view>

          <view class="li verificationLi">
                <label class= "title">总面积</label>
                 <input class= "Number" placeholder-class="phcolor"  value="{{info.ins_area}}"/>
          </view>

           <view class="li verificationLi">
                <label class= "title">场地性质</label>
                <input class= "Number" placeholder-class="phcolor" value='{{info.site_properties}}' />
           </view>

           <view class="li verificationLi">
                <label class= "title">租赁期</label>
                <input class= "Number" placeholder-class="phcolor" value='{{info.leasehold}}' />
           </view>

           <view class="li verificationLi">
                <label class= "title">发票类型</label>
                  <input class= "Number" placeholder-class="phcolor" value='{{info.invoice_type}}'/>
          </view>

          <view class="li_bootom verificationLi">
                <label class= "title">经营年限</label>
                 <input class= "Number" placeholder-class="phcolor" value='{{info.operateyear}}' />
          </view>

           <view class="top_text">
           <view class="distance">机构生源信息</view>   
          </view>
          
          <view class="li verificationLi">
                <label class= "title">教务人数</label>
                 <input class= "Number" placeholder-class="phcolor" value='{{info.edu_num}}' />
          </view>
        
          <view class="top_text">机构生源信息</view>
          
          <view class="li verificationLi">
                <label class= "title">年龄段</label>
                <input class= "Number" placeholder-class="phcolor" value='{{info.agestr}}' />

          </view>

          <view class="li_bootom verificationLi">
              <label class= "title">人数</label>
              <input class= "Number" placeholder-class="phcolor" value='{{info.students}}' />
          </view>

         <view class="top_text">
           <view class="distance">机构教室信息</view>   
          </view>
          
          <view class="li verificationLi">
                <label class= "title">教室数量</label>
                 <input class= "Number" placeholder-class="phcolor"value="{{info.classroom_num}}"/>
          </view>

           <view class="li verificationLi">
                <label class= "title">所在楼层</label>
                <input class= "Number" placeholder-class="phcolor" value='{{info.room_floor}}' />
           </view>

           <view class="li verificationLi">
                <label class= "title">教室长</label>
                <input class= "Number" placeholder-class="phcolor" value="{{info.room_long}}"/>
           </view>

           <view class="li verificationLi">
                <label class= "title">教室宽</label>
                <input class= "Number" placeholder-class="phcolor" value="{{info.room_wide}}" />
           </view>

            <view class="li verificationLi">
                <label class= "title">承重墙</label>
                 <input class= "Number" placeholder-class="phcolor" value="{{info.ins_wall}}"/>
            </view>

           <view class="li verificationLi">
                <label class= "title">网络</label>
                <input class= "Number" placeholder-class="phcolor"  value="{{info.network}}"/>
          </view>

          <view class="li verificationLi">
                <label class= "title">带宽</label>
                <input class= "Number" placeholder-class="phcolor" value="{{info.broadband}}"/>
           </view>
            
             <view class="li_bootom verificationLi">
                <label class= "title">闲置时间</label>
               <input class= "Number" placeholder-class="phcolor" value='{{info.idle_time}}'/>
          </view>

        <view class="top_text">
           <view class="distance">周边信息</view> 
        </view>

       <view class="li verificationLi">
            <label class= "title">交通状况</label>
            <input class= "Number" placeholder-class="phcolor" value="{{info.transport}}"/>
         </view>

         <view class="li verificationLi">
            <label class= "title">基础设施</label>
            <input class= "Number" placeholder-class="phcolor"  value="{{info.infrastructure}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">周边社区</label>
            <input class= "Number" placeholder-class="phcolor"  value="{{info.community}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">常住人口</label>
            <input class= "Number" placeholder-class="phcolor"  value="{{info.inhabitant}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">同类机构</label>
            <input class= "Number" placeholder-class="phcolor" value="{{info.peer_group}}"/>
         </view>
         <view class="li verificationLi">
            <label class= "title">幼儿园</label>
            <input class= "Number" placeholder-class="phcolor"  value="{{info.kindergarten}}"/>
         </view>
    </form>
    
</view>
 <view class='bottom'></view>