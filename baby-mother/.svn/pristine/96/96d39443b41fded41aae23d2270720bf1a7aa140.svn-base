page{
  background: #ffffff;
}
.tab{
  width: 100%;
  font-size:28rpx;
}
.tab view{
  display: inline-block;
  width: 50%;
  text-align: center;
  line-height: 74rpx;
  border-bottom: 2rpx solid #fcfcfe;
  
}
.newUser,.intenUser{
  margin: 0 25rpx;
  padding-bottom: 155rpx;
}
.item{
  height: 154rpx;
  border-bottom: 2rpx solid #fcfcfe;
}
.img_box{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
  margin-top: 17rpx;
}
.img_box image{
  width: 100%;
  max-height: 100%;
}
.text_box{
  display: inline-block;
  padding-left: 30rpx;
}
.title{
  padding-top: 33rpx;
  font-size: 30rpx;
}
.time,.info{
  font-size: 22rpx;
  color: #999999;
  padding-top: 13rpx;
}
.check_box{
  float: right;
  margin-top: 63rpx;
}
.none{
  display: none;
}
.tab .active{
  border-bottom: 4rpx solid #41a638;
  color:#41a638;
}
.send{
  width: 100%;
  height: 155rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #ffffff;
}
.send .btn{
  width: 530rpx;
  height: 80rpx;
  display: inline-block;
  background: #41a638;
  color: #ffffff;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  top: 33rpx;
  margin-left: -265rpx;
}
.send .checkbox{
  position: absolute;
  right: 25rpx;
  top: 33rpx;
}