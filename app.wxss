/**app.wxss**/
page{width:100%;height:100%;background:#eee;font-size:12px;}






view{margin:0;padding:0;}
@font-face {






	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-style: normal;
}
.element_hidden{display: none;}
.element_show{display: block;}





/* 安卓手机隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}


