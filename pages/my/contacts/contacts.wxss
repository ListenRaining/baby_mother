.backg {
  background-color: white;
  height:100%;
}

.nav {
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: solid 5rpx #f2f2f2;
}

.tab {
  height: 80rpx;
  width: 140rpx;
  font-size: 30rpx;
  color: rgb(139, 139, 139);
  display: flex;
  align-items: center;
  text-align: center;
  /* XXX 两个像素的偏差，不然有视觉误差*/
  margin-bottom: -3rpx;
  justify-content: space-around;
}

.underline {
  color: #5fb157;
  border-bottom: solid 5rpx #5fb157;
}

.infor {
  min-height: 166rpx;
  display: flex;
  width:100%;
  align-items: center;
  border-bottom: solid 1rpx #eee;
}

.panel {
  background-color: white;
  width: 100%;
}

.infor image {
  width: 120rpx;
  height: 120rpx;
  margin-left: 32rpx;
  margin-right: 40rpx; 
  border-radius: 50%;
}

.text_panel1 {
  height: 82rpx;
  width: 476rpx;
}

.text_panel1 .uname {
  margin-left: 24rpx;
  font-size: 32rpx;
}

.text_panel1 .time {
  margin-top: 8rpx;
  margin-left: 24rpx;
  font-size: 28rpx;
}

.text_panel2 {
  height: 184rpx;
  width: 476rpx;
}

.text_panel2 .uname {
  margin-left: 24rpx;
  margin-top:10rpx;
  font-size: 32rpx;
}

.text_panel2 .uname4 {
  margin-left: 24rpx;
  margin-top:30rpx;
  font-size: 32rpx;
}

.text_panel2 .time {
  margin-left: 24rpx;
  margin-top: 4rpx;
  font-size: 28rpx;
}

.record {
  display: flex;
  flex-direction: column;
  margin-bottom: 8rpx;
}
.record text {
  color: #7e7e86;
  font-size: 25rpx;
  line-height: 40rpx;
  padding-left: 33rpx;
}


.register{
  width: 100%;
  height: 1334rpx;
  overflow: hidden;
  background-color: white;
}

.hint_icon{
  height: 445rpx;
  width: 100%;
  overflow: hidden;
  text-align: center;
  padding-top: 200rpx;
}

.hint_icon image{
  display: block;
  margin-top: 58rpx;
  width: 170rpx;
  height: 155rpx;
  margin-left: auto;
  margin-right: auto;
}

.hint_icon .hint_content
{
    padding: 0 110rpx;
}

.hint_icon .hint_content view{
  /*word-break: break-all;*/
  /*text-indent:25px;*/
  /*letter-spacing:0rpx;*/
  font-size: 30rpx;
  margin-top: 36rpx;
  color: #6e6e6b;
}

.head_wrap{
    display: flex;
    align-items:flex-start;
    margin-left: 20rpx;
}

.head_wrap image{
    padding: 0;
    margin: 0;
    border: solid 2rpx #9c9895;
    border-radius: 60rpx;
}

.item_btn {
  margin:40rpx 30rpx 40rpx 20rpx;
}

.item_btn button{
  height: 60rpx;
  line-height: 60rpx;
  width: 86rpx;
  font-size: 28rpx;
  background-color: #FF9900;
  color: white;
  text-align: center;
  font-weight: bold;
}

.identity{
  background-color: #CC9933;
  color: white;
  font-size: 25rpx;
  margin-left: 20rpx;
}