<view class="content_wrap">
    <form bindsubmit='formSubmit'>
      <view class="top_text">周边基本情况</view>
 
          <view class="li verificationLi">
            <label class= "title">交通状况</label>
             <input class= "Number" placeholder-class="phcolor" placeholder="请输入实际交通状况" name="ins_name" value="{{ins_name}}"/>
          </view>

           <view class="li verificationLi">
                <label class= "title">基础设施</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="请输入周边基础设施" name= "code" />
                <view class='top_person_2_flex1'>
                    <image src="../../../image/icon-play/arrow.png"></image>
                  </view>
          </view>

          <view class="li verificationLi">
                <label class= "title">周边社区</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="请选择周边社区情况" name= "code" />
                <view class='top_person_2_flex1'>
                    <image src="../../../image/icon-play/arrow.png"></image>
                  </view>
          </view>

           <view class="li verificationLi">
                <label class= "title">常住人口</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="请选择合适人口范围" name= "code" />
                <view class='top_person_2_flex1'>
                    <image src="../../../image/icon-play/arrow.png"></image>
                  </view>
          </view>
          

          <view class="li verificationLi">
                <label class= "title">同类机构</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="0代表没有" name= "code" />
                <view class='top_person_2_flex1 '>
                    个
                  </view>
          </view>

           <view class="li verificationLi">
                <label class= "title">幼儿园</label>
                 <input class= "Number" placeholder-class="phcolor" placeholder="0代表没有" name= "code" />
                <view class='top_person_2_flex1 '>
                    个
                  </view>
          </view>

         
        <button class='.button' form-type='submit'>提交</button>
    </form>
</view>