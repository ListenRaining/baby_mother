page {
  background-color: #f0eff5;
}

.register{
  width: 100%;
  height: 1334rpx;
  overflow: hidden;
  background-color: #f0eff4;
}

.hint_icon{
  height: 445rpx;
  width: 100%;
  overflow: hidden;
  text-align: center;
  padding-top: 200rpx;
}

.hint_icon image{
  display: block;
  margin-top: 58rpx;
  width: 170rpx;
  height: 155rpx;
  margin-left: auto;
  margin-right: auto;
}

.hint_icon .hint_content
{
    padding: 0 110rpx;
}
.hint_icon .hint_content view{
  font-size: 30rpx;
  margin-top: 36rpx;
  color: #6e6e6b;
}

.li{
    display: flex;
    padding: 30rpx 40rpx;
    align-items: center;
    border-bottom: solid 1rpx #d5d5d5;
}

.headIcon{
    width: 130rpx;
    border-radius: 60rpx;
}

.msg{
    display: flex;
    flex-flow: column;
    margin-left: 20rpx;
}

.msg .name{
    font-size: 28rpx;
    display: flex;
    align-items: center;
    width:370rpx;
    height:40rpx;
    overflow:hidden;
}

.msg .type{
    margin-left: 10rpx;
    width: 122rpx;
    height: 34rpx;
    line-height: 34rpx;
    font-size: 26rpx;
    color: #fff;
    text-align: center;
    border-radius: 5rpx;
}

.addFrom, .time{
    font-size: 26rpx;
    color: #87868d;
    width:370rpx;
    height:40rpx;
    overflow:hidden;
}

.associationCnt{
    /* margin-top: -10rpx; */
    margin-left: auto;
    width: 110rpx;
    height: 50rpx;
    font-size: 30rpx;
    line-height: 50rpx;
    text-align: center;
    color: #ffffff;
    background-color: #51d55f;
    border-radius: 8rpx;
}

.associationCntdis{
  /* margin-top: -10rpx; */
  margin-left: auto;
  width: 110rpx;
  height: 50rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  text-align: center;
  color: #ffffff;
  background-color:#9c9895;
  border-radius: 8rpx;
}

.phoneNum{
  font-size:26rpx;
}